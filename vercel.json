{
    "rewrites": [
        {
            "source": "/api/solapi/:path*",
            "destination": "https://api.solapi.com/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "crons": [
        {
            "path": "/api/cron",
            "schedule": "0 0 * * *"
        }
    ]
}